<%= form_with(model: assignee) do |form| %>
  <% if assignee.errors.any? %>
    <div id="error_explanation">
      <h2><%= t("helpers.pluralize.other", count: assignee.errors.count) %></h2>
      <ul>
        <% assignee.errors.each do |error| %>
          <ul><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :user_id %>
    <%= form.text_field :user_id %>
  </div>

  <div class="field">
    <%= form.label :issue_id %>
    <%= form.text_field :issue_id %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
