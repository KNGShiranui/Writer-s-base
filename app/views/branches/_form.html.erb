<!-- 変更後のHTML -->
<%= form_with(model: branch) do |form| %>
  <% if branch.errors.any? %>
    <div id="error_explanation" class="error-messages">
      <h2><%= t("helpers.pluralize.other", count: branch.errors.count) %></h2>
      <ul>
        <% branch.errors.each do |error| %>
          <ul><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <%# <div class="field"> %>
    <%#= form.label :repository_id %>
    <%#= form.text_field :repository_id %>
  <%# </div> %>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
